{
  "build": {
    "additional_pages": {},
    "path_prefix": "/rolling-deployer/",
    "static_dir": "assets"
  },
  "components": {
    "changelog": true,
    "funding": {
      "preferred_funding": "github"
    },
    "artifacts": {
      "cargo_dist": true,
      "package_managers": {
        "preferred": {
          "npm": "npm install -g rolling-deployer",
          "npx": "npx rolling-deployer -h",
          "crates.io": "cargo install rolling-deployer",
          "homebrew": "brew install rolling-deployer"
        }
      }
    }
  },
  "styles": {
    "theme": "axodark"
  }
}